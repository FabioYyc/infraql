--
-- create VPC 
--
INSERT /*+ AWAIT  */ INTO compute.networks
(
 project,
 data__name,
 data__autoCreateSubnetworks,
 data__routingConfig
) 
SELECT
'{{ .global.project }}',
'{{ .network.name }}',
{{ .network.autoCreateSubnetworks }},
'{{ .network.routingConfig }}'
;

SELECT 
  name 
FROM 
  compute.networks 
WHERE 
  project = '{{ .global.project }}'
;

DELETE /*+ AWAIT  */ FROM compute.networks
WHERE 
  project = '{{ .global.project }}'
  AND
  network = '{{ .network.name }}'
;
